{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch SPA",
            "url": "https://www.authsamples-dev.com/",
            "webRoot": "${workspaceRoot}/src/*",
            "sourceMaps": true,
            "sourceMapPathOverrides":{
                "webpack:///./*": "${workspaceRoot}/spa/dist/*"
            }
        }
    ]
}