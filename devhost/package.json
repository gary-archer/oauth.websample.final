  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
    "cookie-encrypter": "^1.0.1",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "tunnel-agent": "^0.6.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/fs-extra": "^9.0.4",
    "@types/node": "^14.14.10",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.21.0",
    "nodemon": "^2.0.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.2.2"
  }
